page{
  background-color:#b3d4db;
}
.container{
  display:flex;
  flex-direction: column;
  align-items: center;
}
.user-avatar{
  width:200rpx;
  height:200rpx;
  border-radius:50%;
  margin-top:160rpx;
}
.user-name{
  font-size: 32rpx;
  font-weight: bold;
  margin-top:10rpx;
}
.moto{
  font-size:22rpx;
  font-weight: bold;
  line-height:80rpx;
}
.moto-container{
  margin-top:200rpx;
  border:1rpx solid #405f80;
  height:80rpx;
  width:200rpx;
  border-radius:10rpx;
  text-align: center;
}